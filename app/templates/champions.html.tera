<!doctype html>
<html>
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>

    <link rel="stylesheet" href="css/style.css"/>
    <!-- <script type="text/javascript" src="./js/search.js"></script>
    <script type="text/javascript" src="./js/champion.js"></script>-->

    <title>ExoBuilds</title>
  </head>
  <body>
    <nav class="navbar fixed-top navbar-dark bg-dark">
      <div class="container-fluid">
        <a class="navbar-brand" href="http://127.0.0.1:8000/"><img src="images/ExoBuilds.svg" height="50px" alt="Home"></a>
      </div>
    </nav>
    <div class="row" style="margin-top: 4rem;">
      <div class="col-1"></div>
      <div class="col-10">
        <div class="row">
          <div class="col-1"></div>
          <div class="col-2" style="margin-top: 4rem;">
            <img src="http://ddragon.leagueoflegends.com/cdn/12.11.1/img/champion/{{ name }}.png" alt="{{ name }}">
          </div>
          <div class="col-9" style="margin-top: 6rem;">
            <h1 style="font-weight:bold">{{ name }} - The darkin blade</h1>
          </div>
        </div>
        <div class="row">
          <div class="col-1"></div>
          <div class="col-2" style="margin-top: 4rem;margin-left:1.4rem">
            <img src="images/jungle.png" alt="Jungle">
          </div>
          <div class="col-6" style="margin-top: 4rem;display:flex">
            <div>
              <img src="http://ddragon.leagueoflegends.com/cdn/12.11.1/img/spell/FlashFrost.png" alt="Spell0">
              <div>
                <span>A</span>
              </div>
            </div>
            <img src="images/arrow.svg" height="50rem" alt="Spell0" style="margin-top: 0.5rem;">
            <div>
              <img src="http://ddragon.leagueoflegends.com/cdn/12.11.1/img/spell/FlashFrost.png" alt="Spell0">
              <div>
                <span>Z</span>
              </div>
            </div>
            <img src="images/arrow.svg" height="50rem" alt="Spell0" style="margin-top: 0.5rem;">
            <div>
              <img src="http://ddragon.leagueoflegends.com/cdn/12.11.1/img/spell/FlashFrost.png" alt="Spell0">
              <div>
                <span>E</span>
              </div>
            </div>
            <img src="images/arrow.svg" height="50rem" alt="Spell0" style="margin-top: 0.5rem;">
            <div>
              <img src="http://ddragon.leagueoflegends.com/cdn/12.11.1/img/spell/FlashFrost.png" alt="Spell0">
              <div>
                <span>R</span>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="col-1"></div>
    </div>
  </body>
</html>